<h1>Insumos Serigrafia Productos</h1>

<table>
  <thead>
    <tr>
      <th>Video</th>
      <th>Titulo</th>
      <th>Linea producto</th>
      <th>Marca</th>
      <th>Descripcion</th>
      <th>Especificaciones tecnicas</th>
      <th>Presentacion</th>
      <th>Precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insumos_serigrafia_productos.each do |insumos_serigrafia_producto| %>
      <tr>
        <td><%= insumos_serigrafia_producto.video %></td>
        <td><%= insumos_serigrafia_producto.titulo %></td>
        <td><%= insumos_serigrafia_producto.linea_producto %></td>
