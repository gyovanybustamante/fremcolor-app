        <td><%= insumos_serigrafia_producto.precio %></td>
        <td><%= link_to 'Show', insumos_serigrafia_producto %></td>
        <td><%= link_to 'Edit', edit_insumos_serigrafia_producto_path(insumos_serigrafia_producto) %></td>
        <td><%= link_to 'Destroy', insumos_serigrafia_producto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
